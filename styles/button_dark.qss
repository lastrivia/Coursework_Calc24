QPushButton {
    height: 32px;
    width: 120px;
    border: none;
    background-color: rgba(47, 47, 57, 1.0);
    border-radius: 8px;
    color: rgba(255, 255, 255, 1.0);
    font-size: 14px;
}
QPushButton:hover {
    background-color: rgba(66, 66, 76, 1.0);
}
QPushButton:pressed {
    background-color: rgba(66, 66, 76, 1.0);
    color: rgba(255, 255, 255, 0.5);
}
QPushButton:disabled {
    color: rgba(255, 255, 255, 0.5);
}
