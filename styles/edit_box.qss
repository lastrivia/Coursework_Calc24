QLineEdit {
    height: 28px;
    border: 1px solid rgba(204, 204, 204, 1.0);
    border-radius: 8px;
    background-color: rgba(255, 255, 255, 1.0);
    padding: 0 8px;
    font-size: 12px;
    color: black;
}
QLineEdit:hover {
    border: 1px solid rgba(253, 251, 250, 1.0);
    background-color: rgba(253, 251, 250, 1.0);
}
QLineEdit:focus {
    border: 1px solid rgba(204, 204, 204, 1.0);
    background-color: rgba(253, 251, 250, 1.0);
}
